{
  "manifest_version": 3,
  "name": "Jelly Truck Chrome Extension",
  "description": "Play Jelly Truck",
  "version": "1.0",
  "icons": {
    "16": "/images/icon16.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "action": {
    "default_icon": "/images/icon128.png",
    "default_popup": "index.html",
    "default_title": "Jelly Truck"
  },
  "permissions": [
    "activeTab"
  ]
}
